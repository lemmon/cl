{
    "name": "lemmon/cl",
    "description": "Console logger for PHP that dumps cleanly to stdout, keeping HTTP responses intact while showing rich variable details.",
    "license": "MIT",
    "type": "library",
    "keywords": [
        "logging",
        "console",
        "debug",
        "development",
        "php",
        "logger",
        "debugging"
    ],
    "authors": [
        {
            "name": "Jakub PelÃ¡k",
            "email": "jpelak@gmail.com"
        }
    ],
    "homepage": "https://github.com/lemmon/cl",
    "require": {
        "php": ">=7.4",
        "symfony/var-dumper": "^6.0 || ^7.0"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.48",
        "phpunit/phpunit": "^9.0 || ^10.0"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "autoload": {
        "files": [
            "src/cl.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Lemmon\\Cl\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "ergebnis/composer-normalize": true
        }
    },
    "scripts": {
        "example": "php -S localhost:8000 examples/simple-server.php",
        "test": "vendor/bin/phpunit"
    }
}
